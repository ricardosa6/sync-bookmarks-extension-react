{
  "name": "Sync bookmarks cross browsers",
  "version": "0.0.1",
  "manifest_version": 3,
  "permissions": ["storage", "bookmarks", "scripting", "identity"],
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "src/scripts/service-worker.ts"
  },
  "icons": {
    "16": "public/16.png",
    "32": "public/32.png",
    "48": "public/48.png",
    "128": "public/128.png"
  }
}
